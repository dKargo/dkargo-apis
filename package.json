{
  "name": "dkargoapis",
  "version": "1.0.0",
  "description": "dkargo api server",
  "main": "index.js",
  "config": {
    "apiport": "10001",
    "service": "0x75Bcc537fe91bfEdacFf68ab4bFF4924CcF24AAf",
    "servicestart": "13401"
  },
  "scripts": {
    "start-apis": "node api/server.js $npm_package_config_apiport",
    "start-mon": "node api/eventMonitor.js $npm_package_config_service $npm_package_config_servicestart",
    "stop-apis": "pkill -f api/server.js",
    "stop-mon": "pkill -f api/eventMonitor.js",
    "start": "npx concurrently \"npm run start-apis\" \"npm run start-mon\"",
    "stop": "npx concurrently \"npm run stop-apis\" \"npm run stop-mon\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "dkargo",
    "api",
    "logistics",
    "blockchain"
  ],
  "author": "jhhong",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "callsites": "^3.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "ethereumjs-common": "^1.5.1",
    "ethereumjs-tx": "^2.1.2",
    "mongoose": "^5.9.27",
    "web3": "^1.2.9",
    "web3-utils": "^1.2.9",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xlsx": "^0.16.3"
  }
}
